<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FinalProject.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FinalProject"
    Shell.FlyoutBehavior="Disabled">
    <TabBar>
        <ShellContent
        Title="Search"
        ContentTemplate="{DataTemplate local:SearchPage}"
        Route="SearchPage" />
        <ShellContent
        Title="On Hold"
        ContentTemplate="{DataTemplate local:OnHoldPage}"
        Route="OnHoldPage" />
        <ShellContent
        Title="Check Out"
        ContentTemplate="{DataTemplate local:CheckoutPage}"
        Route="CheckoutPage" />
        
    </TabBar>
    

</Shell>
